<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.kylew.randomrestaurant.ListRandomActivity">
    <TextView
        android:id="@+id/RandtextView"
        android:layout_width="149dp"
        android:layout_height="60dp"
        android:textSize="18sp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="146dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline13"
        android:layout_marginBottom="2dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline15"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="146dp"
        app:layout_constraintHorizontal_bias="0.088"
        tools:layout_editor_absoluteY="42dp"
        tools:layout_editor_absoluteX="120dp" />

    <Button
        android:id="@+id/backButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Back to List"
        app:layout_constraintHorizontal_bias="0.059"
        app:layout_constraintLeft_toLeftOf="@+id/guideline14"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="7dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline16"
        android:layout_marginLeft="8dp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline13"
        app:layout_constraintGuide_begin="95dp"
        android:orientation="vertical" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline14"
        app:layout_constraintGuide_begin="6dp"
        android:orientation="vertical" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline15"
        app:layout_constraintGuide_begin="104dp"
        android:orientation="horizontal"
        tools:layout_editor_absoluteY="104dp"
        tools:layout_editor_absoluteX="0dp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline16"
        app:layout_constraintGuide_begin="176dp"
        android:orientation="horizontal" />

    <Button
        android:id="@+id/NewRandButton"
        android:layout_width="132dp"
        android:layout_height="62dp"
        android:layout_marginBottom="0dp"
        android:layout_marginRight="0dp"
        android:text="Different Restaurant?"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/guideline16"
        app:layout_constraintRight_toLeftOf="@+id/guideline22"
        tools:layout_editor_absoluteX="184dp"
        tools:layout_editor_absoluteY="104dp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline22"
        app:layout_constraintGuide_begin="316dp"
        android:orientation="vertical" />

</android.support.constraint.ConstraintLayout>
